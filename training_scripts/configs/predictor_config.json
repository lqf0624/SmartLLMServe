{
  "model_type": "DLinear",
  "sequence_length": 120,
  "prediction_horizon": 3,
  "batch_size": 64,
  "learning_rate": 0.001,
  "epochs": 150,
  "channels": 3,
  "individual": true,
  "kernel_size": 25,
  "early_stopping_patience": 15,
